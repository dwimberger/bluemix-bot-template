{
  "name": "wilie-bot",
  "description": "Wiener Linien telegram bot",
  "version": "1.0.0",
  "author": "dieter@wimpi.net",
  "contributors": [
    {
      "name": "Dieter Wimberger",
      "email": "dieter@wimpi.net"
    }
  ],
  "scripts": {
    "start": "node --optimize_for_size --max_old_space_size=96 --gc_interval=100 app/app.js",
    "lint": "./node_modules/.bin/jshint app --reporter=./node_modules/jshint-stylish",
    "checkStyle": "./node_modules/.bin/jscs app",
    "pretest": "npm run-script lint && npm run-script checkStyle && npm run-script docs",
    "test": "./node_modules/.bin/istanbul cover --include-all-sources ./node_modules/.bin/jasmine",
    "docs": "./node_modules/.bin/jsdoc -c ./jsdoc-conf.json",
    "predeploy": "echo I am about to deploy",
    "deploy": "cf push iiw-bot",
    "postdeploy": "echo I have deployed"
  },
  "repository": {
    "type": "git",
    "url": "https://dwimberger@bitbucket.org/dwimberger/iiw-bot.git"
  },
  "bugs": {
    "url": "https://bitbucket.org/dwimberger/iiw-bot/issues"
  },
  "keywords": [
    "iiw",
    "ingress",
    "telegram",
    "bot"
  ],
  "dependencies": {
    "async": "^1.5.2",
    "bluebird": "^3.1.1",
    "cfenv": "1.0.x",
    "csv": "^0.4.6",
    "debug": "^2.2.0",
    "express": "^4.13.3",
    "hashmap": "^2.0.4",
    "moment": "^2.11.0",
    "mongodb": "^2.1.3",
    "node-telegram-bot-api": "^0.19.0",
    "request": "^2.67.0",
    "through2": "^2.0.0"
  },
  "devDependencies": {
    "ink-docstrap": "^1.1.0",
    "istanbul": "^0.4.1",
    "jasmine": "^2.4.1",
    "jasmine-reporters": "^2.0.8",
    "jscs2": "^1.13.2",
    "jsdoc": "^3.4.0",
    "jshint": "^2.8.0",
    "jshint-stylish": "^2.1.0"
  },
  "jscsConfig": {
    "preset": "google",
    "jsDoc": {
      "checkAnnotations": "jsdoc3",
      "checkTypes": "strictNativeCase",
      "extra": {
        "module": false
      }
    }
  },
  "engines": {
    "node": "0.12.x"
  },
  "license": "MIT"
}
